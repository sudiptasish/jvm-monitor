<?xml version="1.0" encoding="UTF-8"?>
<monitor-app>
    <debug-config>
        <enable-process-debug>false</enable-process-debug>
        <debug-port>9998</debug-port>
        <launcher></launcher>
        <cmd-opt>java</cmd-opt>
    </debug-config>
    <transport-config>
        <transport-host>localhost</transport-host>
        <transport-port>9791</transport-port>
        <smtp-host></smtp-host>
    </transport-config>
    <app-config>
        <app-id>0</app-id>
        <app-name>This JVM</app-name>
        <monitor-area>
            <monitor-memory>true</monitor-memory>
            <monitor-memorypool>true</monitor-memorypool>
            <monitor-thread>true</monitor-thread>
            <monitor-gc>true</monitor-gc>
            <monitor-class>true</monitor-class>
            <monitor-cpu>true</monitor-cpu>
            <monitor-mbean>true</monitor-mbean>
        </monitor-area>
        <app-credentials>
            <server-name>localhost</server-name>
            <server-port>0</server-port>
            <user-credentials>
                <user-name></user-name>
                <user-password></user-password>
                <mbean-server></mbean-server>
                <user-role></user-role>
                <role-password></role-password>
            </user-credentials>
        </app-credentials>
        <config-details>
            <enable-ssl>false</enable-ssl>
            <enable-logging>false</enable-logging>
            <enable-mailing>false</enable-mailing>
            <mail-config>
                <mail-cause>MEMORY_THRESHOLD_EXCEEDED</mail-cause>
                <mail-from></mail-from>
                <mail-to></mail-to>
                <mail-cc></mail-cc>
            </mail-config>
            <enable-auto-persistence>false</enable-auto-persistence>
            <persistence>
                <persistence-file-path></persistence-file-path>
                <persistence-interval></persistence-interval>
                <interval-type></interval-type>
            </persistence>
            <enable-auto-connect>false</enable-auto-connect>
            <enable-independent-startup>true</enable-independent-startup>
            <vm-args>
                <min-param>16M</min-param>
                <max-param>32M</max-param>
            </vm-args>
        </config-details>
    </app-config>
</monitor-app>